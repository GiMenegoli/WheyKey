<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.menegoli.wheykey">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_whey2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_whey2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".activity.LoginActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.PerfilActivity"></activity>
        <activity android:name=".activity.CadastroActivity"></activity>
        <activity android:name=".activity.CheckoutActivity"></activity>
        <activity android:name=".activity.CheckoutFinishActivity"></activity>
        <activity android:name=".activity.CheckoutSelectActivity"></activity>
        <activity android:name=".activity.SearchActivity"></activity>
        <activity android:name=".activity.MapaActivity"></activity>
        <activity android:name=".activity.SettingsActivity"></activity>
        <activity android:name=".activity.BarActivity"></activity>



        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>